<p id="notice"><%= notice %></p>

<h1>Listing Service Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_types.each do |service_type| %>
      <tr>
        <td><%= service_type.type_name %></td>
        <td>$<%= service_type.type_code %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
